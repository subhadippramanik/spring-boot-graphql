type Employee {
  id: Int!
  name: String!
  email: String
}

type Query {
  employee(id: Int!): Employee
  employees(name: String, email: String): [Employee]
}

type Mutation {
  addEmployee(name: String!, email: String): Employee
  updateEmployee(id: Int!, name: String, email: String): Employee
}

schema {
  query: Query
  mutation: Mutation
}
